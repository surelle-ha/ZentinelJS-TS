{
	"name": "zentinelts",
	"version": "1.0.0",
	"description": "",
	"main": "./server/server.js",
	"keywords": [],
	"author": "Surelle",
	"license": "ISC",
	"scripts": {
		"free-port": "kill-port --port 8800",
		"clean": "rimraf ./server",
		"watch": "depcheck & rimraf ./server && tsc --watch & tscp -w",
		"build": "depcheck & rimraf ./server && tsc & tscp",
		"key:generate": "node -e \"console.log(`Generated Key: ${require('crypto').randomBytes(32).toString('hex')}`);\"",
		"start:dev": "npm run build && cross-env APP_ENV=development nodemon ./server/server",
		"start:prod": "npm run build && cross-env APP_ENV=production nodemon ./server/server",
		"pm2": "pm2 monit",
		"pm2:dev": "npm run build && pm2 start pm2.config.js",
		"pm2:prod": "npm run build && pm2 start pm2.config.js --env production",
		"pm2:restart": "pm2 restart pm2.config.js",
		"pm2:stop": "pm2 stop pm2.config.js",
		"pm2:delete": "pm2 delete pm2.config.js",
		"db:up": "npm run db:create && npm run db:migrate",
		"db:reset": "npm run db:drop && npm run db:up && npx sequelize db:seed:all",
		"db:create": "npx sequelize db:create",
		"db:drop": "npx sequelize db:drop",
		"db:migrate": "npx sequelize db:migrate",
		"db:rollback": "npx sequelize db:migrate:undo",
		"db:rollback:all": "npx sequelize db:migrate:undo:all",
		"db:migrate:create": "npx sequelize-cli migration:generate --name"
	},
	"dependencies": {
		"@faker-js/faker": "^8.4.1",
		"@types/bcrypt": "^5.0.2",
		"@types/chalk": "^2.2.0",
		"@types/cors": "^2.8.17",
		"@types/express-pino-logger": "^4.0.5",
		"@types/figlet": "^1.5.8",
		"@types/helmet": "^4.0.0",
		"@types/jsonwebtoken": "^9.0.6",
		"@types/multer": "^1.4.11",
		"@types/sequelize": "^4.28.20",
		"bcrypt": "^5.1.1",
		"bcryptjs": "^2.4.3",
		"chalk": "^4.1.2",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"ejs": "^3.1.10",
		"express": "^4.19.2",
		"express-async-handler": "^1.2.0",
		"express-pino-logger": "^7.0.0",
		"express-prometheus-middleware": "^1.2.0",
		"express-rate-limit": "^7.4.0",
		"figlet": "^1.7.0",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"luxon": "^3.5.0",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.11.0",
		"nodemailer": "^6.9.14",
		"pino": "^9.3.2",
		"pino-pretty": "^11.2.2",
		"sequelize": "^6.37.3",
		"typescript-tutorial": "file:",
		"uuid": "^10.0.0",
		"zentinelts": "file:"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
