{
	"name": "typescript-tutorial",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"clean": "rimraf ./backend",
		"watch": "depcheck & rimraf ./backend && tsc --watch & tscp -w",
		"build": "depcheck & rimraf ./backend && tsc & tscp",
		"free-port": "kill-port --port 8800",
		"local:dev": "npm run build && cross-env APP_ENV=development nodemon ./backend/server",
		"local:prod": "npm run build && cross-env APP_ENV=production nodemon ./backend/server",
		"db:up": "npm run db:create && npm run db:migrate",
		"db:reset": "npm run db:drop && npm run db:up && npx sequelize db:seed:all",
		"db:create": "npx sequelize db:create",
		"db:drop": "npx sequelize db:drop",
		"db:migrate": "npx sequelize db:migrate",
		"db:rollback": "npx sequelize db:migrate:undo",
		"db:rollback:all": "npx sequelize db:migrate:undo:all",
		"db:migrate:create": "npx sequelize-cli migration:generate --name"
	},
	"keywords": [],
